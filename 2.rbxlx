	local currentCheck: Instance? = object.Parent
	while currentCheck ~= parentGui and currentCheck ~= nil do
		if currentCheck:IsA("GuiObject") and not currentCheck.Visible then
			return false
		end

		currentCheck = currentCheck.Parent
