		if not targetPart:IsDescendantOf(game) then
			for _, info in guis do
				self.GuiSettings[info.SurfaceGui] = nil
				info.SurfaceGui:Destroy()
			end
			self.TargetParts[targetPart] = nil
		end
		for _, info in guis do

