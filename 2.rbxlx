	local xCos = math.cos(rx / 2)
	local xSin = math.sin(rx / 2)
	local yCos = math.cos(ry / 2)
	local ySin = math.sin(ry / 2)
	local zCos = math.cos(rz / 2)
	local zSin = math.sin(rz / 2)

