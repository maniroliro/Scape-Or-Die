	q0 = Normalize(q0)
	local qX, qY, qZ, qW = q0.X, q0.Y, q0.Z, q0.W

	local angle = 2 * math.acos(qW);
	local s = math.sqrt(1 - qW * qW);

	if s < EPSILON then
		return Vector3.new(qX, qY, qZ) * angle
	else
		return Vector3.new(qX / s, qY / s, qZ / s) * angle
	end
