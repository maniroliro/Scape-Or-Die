local PredicateManager = {}
local MT = {}
MT.__index = MT
export type PredicateManager = typeof(setmetatable({} :: fields, MT))

local logger = Logger.new(script.Name)

-- CONSTANTS --

-----------------------------
-- PRIVATE FUNCTIONS --
