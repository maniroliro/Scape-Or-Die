		handledPlayer[player] = true
		
		-- Run the callback in protected mode:
		local success, cleanup = xpcall(function(player)
			local clean = callback(player)
			if clean ~= nil then
				assert(typeof(clean) == "function", "callback must return a function or nil")
			end
