		if box.left or box.right then
			return true
		end
		assert(box.part ~= nil and box.part:IsA("Part"), "invalid part type, expected Part")
		
		-- Is a leaf node
		if not Geometry.isPointInShape(point, box.part) then
			return false
		end
		
