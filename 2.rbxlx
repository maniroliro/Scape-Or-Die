				end
				debug.profileend()
			end


		end
		Forces = {}
		task.synchronize()
		workspace:BulkMoveTo(Parts, CFrames, Enum.BulkMoveMode.FireCFrameChanged)
		ApplyProperties(Properties)
		for Part, Toggle in TrailFixes do
